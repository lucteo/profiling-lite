# Textual trace format
#
# Commands:
#
# THREAD, tid, string
# LOCATION, locid, name, function, file, line
# ZBEGIN, tid, timestamp, locid
# ZEND, tid, timestamp
# ZPARAM, tid, name, value
# ZTEXT, tid, text
# ZFLOW, tid, flowid

THREAD, 100, "Main thread"
THREAD, 101, "Background thread"
LOCATION, 1, "Request generation", "my_fun1", "test_file.cpp", 1
LOCATION, 2, "Process background result", "my_fun2", "test_file.cpp", 2
LOCATION, 3, "Background work", "my_fun3", "test_file.cpp", 3
ZBEGIN, 100,200,1
ZPARAM, 100, "bool value", true
ZPARAM, 100, "int value", 123
ZPARAM, 100, "double value", 3.141592
ZPARAM, 100, "string value", "Hello, world!"
ZFLOW, 100,1234
ZEND, 100,300
ZBEGIN, 100,400,2
ZFLOW, 100,1234
ZEND, 100,500
ZBEGIN, 101,310,3
ZFLOW, 101,1234
ZEND, 101,385
